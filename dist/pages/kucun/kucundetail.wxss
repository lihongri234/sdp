.btn_box {
    padding: 35rpx 30rpx;
}

.btn_box view {
    width:30%;
    height: 66rpx;
    line-height: 66rpx;
    border-radius: 50px;
    margin-bottom: 30rpx;
    border: 1px solid #c2c2c2;
}
.btn_box view.none{
